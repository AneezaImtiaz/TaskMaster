{
  "name": "taskmaster",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "cleanNpm": "rm -rf package-lock.json node_modules/ && npm cache clean --force && npm i --legacy-peer-deps",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "clear-storybook-cache": "rm -rf node_modules/.cache/storybook",
    "build-storybook": "npm run clear-storybook-cache && storybook build",
    "lint": "eslint \"src/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,vue}\"",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss,md}\""
  },
  "dependencies": {
    "pinia": "^2.2.1",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.35",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@cypress/vite-dev-server": "^5.1.1",
    "@cypress/vue": "^6.0.1",
    "@cypress/webpack-dev-server": "^3.10.1",
    "@eslint/js": "^9.9.0",
    "@storybook/addon-docs": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@storybook/vue3": "^8.2.9",
    "@storybook/vue3-vite": "^8.2.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.2.0",
    "@types/testing-library__dom": "^7.5.0",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "cypress": "^13.13.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "storybook": "^8.2.9",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.0",
    "vue-tsc": "^2.0.29"
  }
}
